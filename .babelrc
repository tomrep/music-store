{
    "presets": [
      "@babel/react",
        [
            "@babel/preset-env",
            {
                "modules": "auto"
            }
        ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      ["import", {
        "libraryName": "antd",
        "style": "true"
      }]
    ]
}
